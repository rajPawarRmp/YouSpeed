{
    "name": "YouSpeed",
    
    "version": "1.0",
    "manifest_version": 3,
    
    "content_scripts": [
        {
          "matches": ["*://www.youtube.com/*"],
          "js": ["popup.js"]
        }
      ],
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "logo.png"
       
    }
}